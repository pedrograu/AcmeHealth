<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- 
			Add your own converters using entries of the following form: 
			<bean class="converters.Class1ToClass2" /> 
		-->
		<bean class="converters.StringToAdministratorConverter" />
		<bean class="converters.AdministratorToStringConverter" />
		
		<bean class="converters.StringToAppointmentConverter" />
		<bean class="converters.AppointmentToStringConverter" />
		
		<bean class="converters.StringToAuthorityConverter" />
		<bean class="converters.AuthorityToStringConverter" />
		
		<bean class="converters.StringToCommentConverter" />
		<bean class="converters.CommentToStringConverter" />
		
		<bean class="converters.StringToCustomerConverter" />
		<bean class="converters.CustomerToStringConverter" />
		
		<bean class="converters.StringToTimetableConverter" />
		<bean class="converters.TimetableToStringConverter" />
	
		
		<bean class="converters.StringToMedicalHistoryConverter" />
		<bean class="converters.MedicalHistoryToStringConverter" />
		
		<bean class="converters.StringToMessageConverter" />
		<bean class="converters.MessageToStringConverter" />
		
		<bean class="converters.StringToPatientConverter" />
		<bean class="converters.PatientToStringConverter" />
		
		<bean class="converters.StringToPrescriptionConverter" />
		<bean class="converters.PrescriptionToStringConverter" />
		
		<bean class="converters.StringToProfileConverter" />
		<bean class="converters.ProfileToStringConverter" />
		
		<bean class="converters.StringToOfferConverter" />
		<bean class="converters.OfferToStringConverter" />
		
		
		<bean class="converters.StringToSpecialistConverter" />
		<bean class="converters.SpecialistToStringConverter" />
		
		<bean class="converters.StringToSpecialtyConverter" />
		<bean class="converters.SpecialtyToStringConverter" />
		
		<bean class="converters.StringToUserAccountConverter" />
		<bean class="converters.UserAccountToStringConverter" />
		
		
	</util:list>
	
</beans>